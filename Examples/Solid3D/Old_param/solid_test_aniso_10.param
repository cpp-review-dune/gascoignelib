linearsolve		gmres

nonlinear_maxiter	40
nonlinear_tol		1.e-8
nonlinear_globaltol	1.e-8
linear_tol		1.e-4
linear_maxiter		100
linear_globaltol	1.e-19

mgtype            V

nonlinear_rho		0.1
nonlinear_damp 	6
nonlinear_increase	1.e6

gmressize		100
mgomega	1.0
//Block Solver

omega 	1.0

matrixtype block

iterpre                 5
iterpost                5
iterexact               5

discname       Q2

ilum 3 0.05 0.05 0.05

ndirect        40

//Block Visualization
showoutput 0
gnua	1
