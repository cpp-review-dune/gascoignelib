pressure    1000
//mat_law artery
mat_law STVK


//Block Loop

writevtk	0
writebubgup 	0

niter       1

//Block Mesh

dimension   3
prerefine  1
//gridname    solid_coarse.inp
//gridname    Verzweigung_FSI_neu_solid.inp
gridname    solid_10.inp
etapatcher 0


//Block MultiLevelSolver

linear_pstep		0
